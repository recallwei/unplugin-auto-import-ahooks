{
  "include": ["build.config.ts", "src"],
  "compilerOptions": {
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "noUnusedLocals": true,
    "esModuleInterop": true
  }
}
